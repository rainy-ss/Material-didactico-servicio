{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi":["error","always"],
        "indent":["error", 4],
        "react/self-closing-comp": ["error", {
            "component": true,
            "html": true
          }],
        "no-console":"warn"
    }
}
